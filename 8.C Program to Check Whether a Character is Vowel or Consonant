#include <stdio.h>
int main()
{
  char c;
  printf("Enter a character:");
  scanf("%c",&c);
  if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U'){
      printf("Character is vowel:%c\n",c);
  }
  else{
      printf("Character is consonant:%c\n",c);
  }
  
    return 0;

}

